{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "occ/mailchimp",
	"title": "Mailchimp",
	"description": "A Mailchimp form.",
	"textdomain": "rather-simple-mailchimp",
	"category": "embed",
	"attributes": {
		"url": {
			"type": "string"
		},
		"u": {
			"type": "string"
		},
		"id": {
			"type": "string"
		},
		"firstName": {
			"type": "boolean",
			"default": false
		},
		"lastName": {
			"type": "boolean",
			"default": false
		},
		"placeholder": {
			"type": "boolean",
			"default": false
		}
	},
	"supports": {
		"html": false,
		"multiple": false,
		"spacing": {
			"margin": true,
			"padding": true
		}
	},
	"editorScript": "file:./index.js",
	"viewScript": [
		"file:./mc-subscribe.js",
		"file:./view.js"
	],
	"style": "file:./style-index.css",
	"editorStyle": "file:./index.css"
}