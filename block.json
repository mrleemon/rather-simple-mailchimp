{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "occ/mailchimp",
	"title": "Mailchimp",
    "description": "A Mailchimp form.",
	"textdomain": "rather-simple-mailchimp",
	"icon": "email",
	"category": "embed",
    "attributes": {
        "url": {
            "type": "string",
            "source": "attribute",
            "selector": ".message"
        },
        "u": {
            "type": "string",
            "source": "attribute",
            "selector": ".message"
        },
        "id": {
            "type": "string",
            "source": "attribute",
            "selector": ".message"
        },
        "firstName": {
            "type": "boolean",
            "source": "attribute",
            "selector": ".message"
        },
        "lastName": {
            "type": "boolean",
            "source": "attribute",
            "selector": ".message"
        }
    },
	"editorScript": "file:./build/index.js",
    "script": "file:./build/script.js",
    "style": "file:./build/style.css"
}